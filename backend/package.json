{
  "name": "backend",
  "version": "0.0.1",
  "description": "Chat with your PDF using this AI agent",
  "author": "Mayo Oshin",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx tsx src/server.ts",
    "start": "node dist/server-simple.js",
    "build": "tsc",
    "build:prod": "npx tsc src/server-simple.ts src/api/rate-limits-simple.ts --outDir dist --target ES2021 --module NodeNext --moduleResolution nodenext --esModuleInterop && cp -r src/public dist/",
    "clean": "rm -rf dist",
    "demo": "npx tsx demo.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:changed": "jest --onlyChanged --passWithNoTests",
    "test:related": "jest --findRelatedTests",
    "test:coverage": "jest --coverage",
    "test:int": "jest --testPathPattern=\\.int\\.test\\.ts$",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:langgraph-json": "node scripts/checkLanggraphPaths.js",
    "lint:all": "yarn lint & yarn lint:langgraph-json",
    "langgraph:dev": "npx @langchain/langgraph-cli dev"
  },
  "dependencies": {
    "@huggingface/inference": "^4.0.6",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.32",
    "@langchain/langgraph": "^0.2.41",
    "@langchain/langgraph-cli": "^0.0.1",
    "@langchain/langgraph-sdk": "^0.0.36",
    "@mendable/firecrawl-js": "^1.15.7",
    "@supabase/supabase-js": "^2.48.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "sentiment": "^5.0.2",
    "trading-signals": "^6.8.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0",
    "yahoo-finance2": "^2.11.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
}
